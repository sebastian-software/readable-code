{
  "name": "readable-code",
  "version": "3.4.6",
  "description": "JavaScript/CSS Coding Standards and Tooling",
  "main": "lib/index.es5.cjs.js",
  "module": "lib/index.es5.esm.js",
  "main:es2015": "lib/index.es2015.cjs.js",
  "module:es2015": "lib/index.es2015.esm.js",
  "scripts": {
    "test": "eslint --print-config .eslintrc.yml > /dev/null",
    "release": "git push && release-it --github.release --npm.publish --non-interactive",
    "release:minor": "git push && release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "git push && release-it --github.release --npm.publish --non-interactive --increment major",
    "check": "eslint-find-rules -u .eslintrc.yml || true",
    "prepare": "prepublish"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0",
    "yarn": ">=0.21.3"
  },
  "keywords": [
    "lint",
    "es6",
    "eslint",
    "eslintconfig",
    "stylelint",
    "code style",
    "check",
    "quality",
    "standard",
    "gulp",
    "git"
  ],
  "author": {
    "name": "Sebastian Software",
    "email": "s.werner@sebastian-software.de",
    "url": "sebastian-software.de"
  },
  "license": "Apache-2.0",
  "repository": "https://github.com/sebastian-software/readable-code",
  "dependencies": {
    "eslint": "^4.3.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-css-modules": "^2.7.3",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsdoc": "^3.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-lodash": "^2.4.4",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.3.1",
    "flow-bin": "^0.52.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-stylelint": "^4.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "postcss": "^6.0.8",
    "prettier": "^1.5.3",
    "stylelint": "^8.0.0",
    "stylelint-formatter-pretty": "^1.0.3",
    "stylelint-order": "^0.6.0",
    "stylelint-scss": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^3.0.0-beta.3",
    "babel-preset-edge": "^1.1.2",
    "eslint-find-rules": "^3.1.1",
    "prepublish": "^1.2.5"
  }
